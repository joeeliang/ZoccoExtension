{
  "manifest_version": 2,
  "name": "Bionic Reader",
  "version": "1.0",
  "description": "Bolds the first few letters of every word",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}
