{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
    "description": "Change the background color of a webpage.",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }]
  }
  